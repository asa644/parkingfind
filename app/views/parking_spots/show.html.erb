<div class="container text-center">
  <div class="content">
    <p>description: <%= @parking_spot.description %></p>
    <p>city: <%= @parking_spot.city %></p>
    <p>country: <%= @parking_spot.country %></p>
    <p>price: <%= @parking_spot.price %></p>
    <p>number of spots: <%= @parking_spot.number_of_spots %></p>
    <h4><%= link_to "Back", user_parking_spots_path(@user) %></h4>
    <h4><%= link_to "Edit", edit_user_parking_spot_path(@user) %></h4>
    <h4><%= link_to "Delete", user_parking_spot_path(@user), method: :delete %></h4>
  </div>
</div>


