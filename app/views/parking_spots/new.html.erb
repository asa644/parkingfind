<div class="container">
  <h1>New Parking Spot</h1>
  <%= simple_form_for(@parking_spot) do |f| %>
    <%= f.label :description %>
    <%= f.text_field :description %>

    <br><br>
    <%= f.label :city %>
    <%= f.text_field :city %>
    <br><br>

    <%= f.label :country %>
    <%= f.text_field :country %>
    <br><br>

    <%= f.label :price %>
    <%= f.text_field :price %>
    <br><br>

    <%= f.label :number_of_spots %>
    <%= f.number_field :number_of_spots %>
    <br><br>

    <%= f.label :street_address %>
    <%= f.text_field :street_address %>
    <br><br>

    <%= f.input :photo %>
    <%= f.input :photo_cache, as: :hidden %>
    <br><br>

    <%= f.submit %>
  <% end %>
</div>

