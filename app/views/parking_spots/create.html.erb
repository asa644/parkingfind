<div class="container text-center">
  <%= simple_form_for (@parking) do |f| %>
    <%= f.error_notification %>

    <div class="form-inputs">
      <%= f.input :description %>
    </div>

    <div class="form-inputs">
      <%= f.input :city %>
    </div>

    <div class="form-inputs">
      <%= f.input :price %>
    </div>

    <div class="form-inputs">
      <%= f.input :number_of_spots %>
    </div>

    <div class="form-inputs">
      <%= f.input :street_address %>
    </div>


    <div class="form-actions">
      <%= f.button :submit %>
    </div>
  <% end %>
</div>
